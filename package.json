    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react": ">=15.5.4",
    "react-addons-test-utils": ">=15.5.4",
    "react-dom": ">=15.5.4",
    "react-hot-loader": "^4.3.1",
    "react-test-renderer": ">=15.5.4",
    "redbox-react": "^1.6.0",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "sinon": "^5.1.1",
